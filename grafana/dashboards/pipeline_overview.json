{
  "uid": "pipeline-overview",
  "title": "Pipeline Overview",
  "tags": ["de-platform", "overview"],
  "timezone": "utc",
  "schemaVersion": 39,
  "version": 1,
  "editable": true,
  "time": {
    "from": "now-1h",
    "to": "now"
  },
  "refresh": "10s",
  "panels": [
    {
      "id": 1,
      "title": "Ingestion Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 0,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(events_ingested_total[5m])",
          "legendFormat": "{{service}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
            "lineInterpolation": "smooth",
            "fillOpacity": 10,
            "pointSize": 5,
            "showPoints": "auto",
            "spanNulls": false
          },
          "unit": "ops"
        },
        "overrides": []
      },
      "options": {
        "tooltip": {
          "mode": "multi",
          "sort": "desc"
        },
        "legend": {
          "displayMode": "table",
          "placement": "bottom",
          "calcs": ["mean", "max"]
        }
      }
    },
    {
      "id": 2,
      "title": "Normalization Rate",
      "type": "timeseries",
      "gridPos": {
        "h": 8,
        "w": 12,
        "x": 12,
        "y": 0
      },
      "datasource": {
        "type": "prometheus",
        "uid": "prometheus"
      },
      "targets": [
        {
          "refId": "A",
          "expr": "rate(events_processed_total{service=\"normalizer\"}[5m])",
          "legendFormat": "{{status}}",
          "datasource": {
            "type": "prometheus",
            "uid": "prometheus"
          }
        }
      ],
      "fieldConfig": {
        "defaults": {
          "custom": {
            "drawStyle": "line",
